{
  "sourceRoot": "libs/prisma-models/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/prisma-models/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/prisma-models"],
      "options": {
        "jestConfig": "libs/prisma-models/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "migrate": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma migrate dev --name init --schema ./src/prisma/schema.prisma",
        "cwd": "libs/prisma-models"
      }
    },

    "seed": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx esno src/lib/seed && node src/lib/seed.js",
        "cwd": "libs/prisma-models"
      }
    },
    "studio": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma studio --schema ./src/prisma/schema.prisma",
        "cwd": "libs/prisma-models"
      }
    },
    "gen-client": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma generate --schema ./src/prisma/prisma.schema",
        "cwd": "libs/prisma-models"
      }
    }
  }
}
